{
    "name": "codeblocks",
    "version": "0.2.10",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "dev-phaser":"tsc -p ./tsconfig-phaser.json -w",
        "build-phaser":"tsc -p ./tsconfig-phaser.json",
        "build-lib": "vue-cli-service build --entry ./src/main.ts --target lib",
        "build-ilias": "node ilias-builder.js",
        "build-examples": "node examples-builder.js"
    },
    "dependencies": {
        "@quasar/extras": "^1.9.3",
        "core-js": "^3.6.5",
        "highlight.js": "^10.4.1",
        "quasar": "^1.12.13",
        "vue-class-component": "^7.2.5",
        "vue-codemirror": "^4.0.5",
        "vue-i18n": "^8.20.0",
        "vue-uuid": "^2.0.2"
    },
    "devDependencies": {
        "@intlify/vue-i18n-loader": "^1.0.0",
        "@types/jquery": "^3.5.1",
        "@typescript-eslint/eslint-plugin": "^3.8.0",
        "@typescript-eslint/parser": "^3.8.0",
        "@vue/cli": "^4.4.6",
        "@vue/cli-plugin-babel": "^4.4.6",
        "@vue/cli-plugin-typescript": "^4.4.6",
        "@vue/cli-service": "^4.4.6",
        "babel-plugin-transform-imports": "1.5.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-import-resolver-typescript": "^2.2.0",
        "eslint-plugin-html": "^6.0.2",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-json": "^2.1.2",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-vue": "^6.2.2",
        "fibers": "^5.0.0",
        "jquery": "^3.5.1",
        "node-sass": "^4.14.1",
        "postcss-rtl": "^1.5.0",
        "prettier": "^2.0.5",
        "reflect-metadata": "^0.1.13",
        "sass": "^1.26.10",
        "sass-loader": "^9.0.3",
        "shelljs": "^0.8.4",
        "stylus": "^0.54.8",
        "stylus-loader": "^3.0.2",
        "typescript": "^3.9.7",
        "vue": "^2.6.11",
        "vue-cli-plugin-codemirror": "0.0.6",
        "vue-cli-plugin-i18n": "^1.0.1",
        "vue-cli-plugin-quasar": "^2.0.2",
        "vue-property-decorator": "^9.0.0",
        "vue-template-compiler": "^2.6.10"
    },
    "babel": {
        "plugins": [
            [
                "transform-imports",
                {
                    "quasar": {
                        "transform": "quasar/dist/babel-transforms/imports.js",
                        "preventFullImport": true
                    }
                }
            ]
        ],
        "presets": [
            "@vue/cli-plugin-babel/preset"
        ]
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}
