{
    "compilerOptions": {
        "target": "es6",
        "module": "es6",
        "strict": true,
        "noImplicitAny": false,
        "jsx": "preserve",
        "importHelpers": true,
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "esModuleInterop": false,
        "strictBindCallApply": true,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "baseUrl": ".",
        "types": [
            "webpack-env",
            "jquery"
        ],
        "paths": {
            "@/*": [
                "./dev/phaser/*"
            ]
        },
        "lib": [
            "esnext",
            "dom",
            "dom.iterable",
            "scripthost",
        ],
        "outDir": "./public/js/phaser/3.54.0"
    },
    "include": [
        "./dev/phaser/*.ts"
    ],
    "exclude": [
        "node_modules"
    ],
    "typeRoots": ["./dev/phaser/types/"],
    
}