@import './quasar.variables.styl'

.random-tag-placeholder, .template-tag-placeholder
    padding-left:2px
    padding-right:3px
    padding-bottom:3px
    padding-top:0px
    border-radius: 4px
    box-shadow: 2px 2px 3px rgba(0,0,0,0.3)
.random-tag-placeholder
    background-color: rgba(255, 255, 255, 0.75)
    border:1px solid rgba(0,0,0,0.5)
.template-tag-placeholder
    text-shadow:none!important
    background-color: $pink-8
    color:$blue-grey-11!important